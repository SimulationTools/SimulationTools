 RowBox[{"DataTable", "[", 
  RowBox[{
   InterpretationBox[
    RowBox[{"\<\"<\"\>", "\[InvisibleSpace]", "150", 
     "\[InvisibleSpace]", "\<\">\"\>"}],
    SequenceForm["<", 150, ">"],
    Editable->False], ",", 
   RowBox[{"{", 
    RowBox[{"{", 
     RowBox[{"0.`", ",", "300.`"}], "}"}], "}"}]}], "]"}]], "Output",
 CellLabel->"Out[14]="]
